var count = 0;
var stop = false;
while true {
    print("before");
    var i = 2;
    while i > 0 {
        count += 1;
        if count == 7 {
            stop = true;
            break;
        }
        i -= 1;
    }
    if stop {
        break;
    }
    print("after");
}