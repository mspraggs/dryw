// value = {foo} ...
// value = nil ...
// value = 3 ...
// value = true ...
// value = true ...
// value = <built-in fn clock> ...
// value = <fn two @ [MEMADDR]> ...
// value = <class Foo> ...
// value = <Foo instance @ [MEMADDR]> ...
// value = <method foo on <Foo instance @ [MEMADDR]> @ [MEMADDR]> ...
// 0
fn two() { return 2; }

#[constructor(new)]
class Foo {
    fn foo(self) {}
}

print("value = {${"foo"}} ...");
print("value = ${nil} ...");
print("value = ${1 + two()} ...");
print("value = ${true} ...");
print("value = ${!false} ...");
print("value = ${clock} ...");
print("value = ${two} ...");
print("value = ${Foo} ...");
print("value = ${Foo.new()} ...");
print("value = ${Foo.new().foo} ...");