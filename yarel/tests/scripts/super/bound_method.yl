// A.method(arg)
// 0
class A {
  fn method(self, arg) {
    print("A.method(" + arg + ")");
  }
}
#[constructor(new)]
class B < A {
  fn get_closure(self) {
    return super.method;
  }
  fn method(self, arg) {
    print("B.method(" + arg + ")");
  }
}
var closure = B.new().get_closure();
closure("arg");
