class Foo {
  static fn method0() { return "no args"; }
  static fn method1(a) { return a; }
  static fn method2(a, b) { return a + b; }
  static fn method3(a, b, c) { return a + b + c; }
  static fn method4(a, b, c, d) { return a + b + c + d; }
  static fn method5(a, b, c, d, e) { return a + b + c + d + e; }
  static fn method6(a, b, c, d, e, f) { return a + b + c + d + e + f; }
  static fn method7(a, b, c, d, e, f, g) { return a + b + c + d + e + f + g; }
  static fn method8(a, b, c, d, e, f, g, h) { return a + b + c + d + e + f + g + h; }
}
print(Foo.method0());
print(Foo.method1(1));
print(Foo.method2(1, 2));
print(Foo.method3(1, 2, 3));
print(Foo.method4(1, 2, 3, 4));
print(Foo.method5(1, 2, 3, 4, 5));
print(Foo.method6(1, 2, 3, 4, 5, 6));
print(Foo.method7(1, 2, 3, 4, 5, 6, 7));
print(Foo.method8(1, 2, 3, 4, 5, 6, 7, 8));
var foo = Foo();
print(foo.method0());
print(foo.method1(1));
print(foo.method2(1, 2));
print(foo.method3(1, 2, 3));
print(foo.method4(1, 2, 3, 4));
print(foo.method5(1, 2, 3, 4, 5));
print(foo.method6(1, 2, 3, 4, 5, 6));
print(foo.method7(1, 2, 3, 4, 5, 6, 7));
print(foo.method8(1, 2, 3, 4, 5, 6, 7, 8));
